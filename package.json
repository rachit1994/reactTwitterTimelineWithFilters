{
  "name": "ROIHunterFrontendTestingTask",
  "version": "1.0.0",
  "main": "app.js",
  "author": "Rachit <rachit.srivastava@mail.vinove.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn && yarn binary-start",
    "heroku-postbuild": "cd client && yarn && yarn build",
    "client": "cd client && yarn start",
    "server": "nodemon --exec babel-node --presets env -- app.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "binary-start": "./node_modules/.bin/babel-node  app.js"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "dotenv": "^5.0.1",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "rimraf": "^2.6.2",
    "twit": "^2.2.9"
  }
}
